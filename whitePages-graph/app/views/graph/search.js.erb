<% if  @flag == true%>
    $('#wp_graph').replaceWith("<%= escape_javascript render(:partial => 'graph/search',:locals => {:response => @response,:error=> @error}) %>");
    $('#wp_tabs').html("<%= escape_javascript render(:partial => 'shared/tabs',:locals => {:search_for => @search_for}) %>");
    $(".tab-content").hide();
    <% if @search_for == "location"%>
        $("#faddress-tab").show();
    <% elsif @search_for == "phone"%>
        $("#fnumber-tab").show();
    <% elsif @search_for == "person"%>
        $("#fperson-tab").show();
    <% elsif @search_for == "business"%>
        $("#fbusiness-tab").show();
    <% end %>
<% end %>
