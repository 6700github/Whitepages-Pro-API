<div class="detail_wrapper">
  <div class="disp_result_box">
    <table width="100%" border="0" cellspacing="0" cellpadding="0">
      <tr>
        <th align="left" width="30%">Who</th>
        <th align="left" width="30%">Where</th>
      </tr>
      <% @results[:result].each do |person_obj| %>
        <tr>
          <td> <div class="detail_boxin">
            <p class="bg_font14"><%=person_obj[:name]  %></p>
            <%  unless person_obj[:age].blank?%> <p><span class="bg_font12">Age:</span> &nbsp;<%=person_obj[:age]["start"]  %>+</p>  <% end %>
            <%  unless person_obj[:contact_type].blank?%><p><span class="bg_font12">Type:</span>&nbsp;<%=person_obj[:contact_type].first  %></p> <% end %>
          </div></td>
          <td>
            <div class="detail_boxin">
              <% unless person_obj[:address].blank? %>
                <% unless person_obj[:address][:standard_address_line1].blank? %> <%=person_obj[:address][:standard_address_line1]  %> <br /> <% end %>
                <% unless person_obj[:address][:standard_address_line2].blank? %> <%=person_obj[:address][:standard_address_line2]  %> <br /> <% end %>
                <% unless person_obj[:address][:city].blank? %>  <%=person_obj[:address][:city] %>  <% end %>
                <% unless person_obj[:address][:state_code].blank? %>  <%=person_obj[:address][:state_code] %> <% end %>
                <% unless person_obj[:address][:postal_code].blank? %>  <%=person_obj[:address][:postal_code] %>  <% end %>
                <p><span class="bg_font12">Receiving Mail: </span> <%=person_obj[:address][:receiving_mail] %>  </p>
                <% unless person_obj[:address][:usage].blank? %><p><span class="bg_font12">Usage: </span><%=person_obj[:address][:usage] %> </p> <% end %>
                <p><span class="bg_font12">Delivery Point: </span> <%=person_obj[:address][:delivery_point].to_s[0..-5] + " Unit"%> </p>
              <% end %>
            </div>
          </td>
        </tr>
      <% end %>
    </table>
  </div>
</div>